<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TagFlowLayout">
        <attr name="max_select" format="integer"></attr>
        <attr name="tag_gravity">
            <enum name="left" value="-1" />
            <enum name="center" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="AutoScrollRecylerView">
        <attr name="interpolator">
            <enum name="LinearInterpolator " value="0" />
            <enum name="AccelerateInterpolator" value="1" />
            <enum name="DecelerateInterpolator" value="2" />
            <enum name="AccelerateDecelerateInterpolator " value="3" />
        </attr>
        <attr name="speed" format="float" />
        <attr name="itemSpace" format="integer" />
    </declare-styleable>
</resources>